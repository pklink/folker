meta {
  name: create artist
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/artists
  body: json
  auth: none
}

body:json {
  {
    "name": "Pink Floyd"
  }
}

assert {
  res.status: eq 200
  res.body.id: isNotEmpty
  res.body.name: eq Pink Floyd
}

script:post-response {
  bru.set
}

tests {
  test("response has only expected properties", () => {
    const expectedKeys = ["id", "name"];
    expect(res.body).to.have.all.keys("id", "name");
  });
}
